  },
  "proxy": "http://localhost:8000",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "http-proxy-middleware": "^2.0.6",
    "next": "^12.3.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-sortable-hoc": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/qs": "^6.5.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "cypress": "^10.8.0",
    "eslint-plugin-react": "^7.31.8",
    "sass": "^1.55.0",
