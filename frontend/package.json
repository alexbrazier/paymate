{
  "name": "frontend",
  "version": "0.0.0",
  "author": "alexbrazier",
  "private": true,
  "scripts": {
    "dev": "next",
    "start": "yarn dev",
    "start:prod": "next start",
    "build": "next build",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:e2e:local": "cypress open",
    "lint": "eslint \"src/**/*.{j,t}s{x,}\"",
    "prettier": "prettier \"src/**/*.{js,ts,tsx,jsx,css,scss,json,html}\"",
